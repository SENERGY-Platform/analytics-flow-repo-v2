                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Flow"
                ],
                "summary": "Get flows",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.FlowsResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/flow/": {
            "put": {
                "description": "Validates and stores a flow",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "Flow"
                ],
                "summary": "Create flow",
                "parameters": [
                    {
                        "description": "Create flow",
                        "name": "flow",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.Flow"
                        }
                    }
